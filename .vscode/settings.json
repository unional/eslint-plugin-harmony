{
  "cSpell.ignorePaths": [
    ".git/objects",
    ".vscode-insiders",
    ".vscode",
    "CHANGELOG.md",
    "node_modules",
    "package-lock.json",
    "vscode-extension",
    "pnpm-lock.yaml"
  ],
  "cSpell.words": [
    "eslintplugin",
    "singleline",
    "snyk",
    "stroustrup",
    "styleguide",
    "TSES"
  ],
  "eslint.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "json.schemas": [
    {
      "fileMatch": [
        "lib/**/*.json"
      ],
      "url": "http://json.schemastore.org/eslintrc"
    },
    {
      "fileMatch": [
        "tsconfig.*.json"
      ],
      "url": "http://json.schemastore.org/tsconfig"
    }
  ],
  "prettier.eslintIntegration": true,
  "prettier.requireConfig": true
}
