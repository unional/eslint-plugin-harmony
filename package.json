{
  "name": "eslint-plugin-harmony",
  "version": "7.1.1",
  "description": "eslint config that works in harmony with different IDEs",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "javascript",
    "prettier",
    "styleguide",
    "tslint",
    "typescript"
  ],
  "homepage": "https://github.com/unional/eslint-plugin-harmony",
  "bugs": {
    "url": "https://github.com/unional/eslint-plugin-harmony/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/eslint-plugin-harmony"
  },
  "license": "MIT",
  "author": "Unional",
  "main": "lib/index.js",
  "files": [
    "lib",
    "ide-configs"
  ],
  "scripts": {
    "bootstrap": "pnpm build && pnpm link .",
    "build": "tsc -p tsconfig.lib.json",
    "build:watch": "tsc -w",
    "lint": "eslint --ext=js,ts src",
    "nuke": "rimraf lib node_modules",
    "prepare": "husky install",
    "prettier": "prettier spec/ts-prettier/**/*.pass.ts --write",
    "release": "pnpm build && pnpm test && semantic-release",
    "test": "gulp",
    "verify": "pnpm build && pnpm lint && pnpm test"
  },
  "dependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@typescript-eslint/experimental-utils": "^5.62.0",
    "@typescript-eslint/parser": "^6.19.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@types/node": "20.17.1",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "ansi-colors": "4.1.3",
    "eslint": "8.57.1",
    "eslint-config-prettier": "^9.0.0",
    "gulp": "4.0.2",
    "gulp-eslint": "6.0.0",
    "husky": "9.1.6",
    "lodash.uniq": "4.5.0",
    "npm-check-updates": "17.1.8",
    "plugin-error": "2.0.1",
    "prettier": "3.3.3",
    "rimraf": "6.0.1",
    "semantic-release": "^24.0.0",
    "through": "2.3.8",
    "typescript": "5.6.3"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": ">= 6.0.0",
    "eslint": ">=8.4.0",
    "eslint-config-prettier": ">= 9.0.0"
  },
  "peerDependenciesMeta": {
    "eslint-config-prettier": {
      "optional": true
    }
  },
  "packageManager": "pnpm@9.12.3"
}
