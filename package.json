{
  "name": "eslint-plugin-harmony",
  "version": "7.0.1",
  "description": "eslint config that works in harmony with different IDEs",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "javascript",
    "prettier",
    "styleguide",
    "tslint",
    "typescript"
  ],
  "homepage": "https://github.com/unional/eslint-plugin-harmony",
  "bugs": {
    "url": "https://github.com/unional/eslint-plugin-harmony/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/eslint-plugin-harmony"
  },
  "license": "MIT",
  "author": "Unional",
  "main": "lib/index.js",
  "files": [
    "lib",
    "ide-configs"
  ],
  "scripts": {
    "bootstrap": "yarn build && yarn link && yarn link eslint-plugin-harmony",
    "build": "tsc -p tsconfig.lib.json",
    "build:watch": "tsc -w",
    "lint": "eslint --ext=js,ts src",
    "nuke": "rimraf lib node_modules",
    "prepare": "husky install && yarn bootstrap",
    "prettier": "prettier spec/ts-prettier/**/*.pass.ts --write",
    "release": "yarn build && yarn test && standard-version",
    "test": "gulp",
    "verify": "yarn build && yarn lint && yarn test"
  },
  "dependencies": {
    "@typescript-eslint/experimental-utils": "^5.23.0"
  },
  "devDependencies": {
    "@commitlint/cli": "16.3.0",
    "@commitlint/config-conventional": "16.2.4",
    "@types/node": "17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "ansi-colors": "4.1.1",
    "eslint": "8.15.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-harmony": "file:.",
    "gulp": "4.0.2",
    "gulp-eslint": "6.0.0",
    "husky": "8.0.1",
    "lodash.uniq": "4.5.0",
    "npm-check-updates": "12.5.11",
    "plugin-error": "1.0.1",
    "prettier": "2.6.2",
    "rimraf": "3.0.2",
    "standard-version": "9.3.2",
    "through": "2.3.8",
    "typescript": "4.6.4"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": ">=8.4.0",
    "eslint-config-prettier": "^8.0.0"
  },
  "packageManager": "yarn@1.22.18"
}
